idade = int(input("Idade: "))

if (idade >= 5 and idade <= 7):
    print("Infantil A")
elif (idade >= 8 and idade <= 10):
    print("Infantil B")
elif (idade >= 11 and idade <= 13):
    print("Juvenil A")
elif idade >= 14 and idade <= 17:
    print("Juvenil B")
elif (idade >= 18 and idade <= 20):
    print("Senior")
elif (idade > 20):
    print("Master")
else:
    print("Idade invalida")
